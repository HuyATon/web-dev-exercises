<table class="table table-hover mt-4">
  <thead class="table-primary">
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Tên sản phẩm</th>
      <th scope="col">Giá</th>
      <th scope="col">Số lượng</th>
    </tr>
  </thead>
  <tbody>
     {{#each products}}
            <tr>
        <th scope="row"> {{this.id }} </th>
        <td>{{this.name}}</td>
        <td>{{this.price}}</td>
        <td>{{this.quantity}}</td>
        </tr>
    {{/each}}
    
  </tbody>
</table>