<div class="container-fluid bg-dark p-3">
  <div class="mb-3 bg-dark-blue p-2 rounded border-dark-blue">
    <a href="/">
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="32"
        height="32"
        fill="currentColor"
        class="bi bi-house-fill text-light"
        viewBox="0 0 16 16"
      >
        <path
          d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L8 2.207l6.646 6.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293z"
        />
        <path
          d="m8 3.293 6 6V13.5a1.5 1.5 0 0 1-1.5 1.5h-9A1.5 1.5 0 0 1 2 13.5V9.293z"
        />
      </svg>
    </a>
  </div>
  <div class="border border-secondary rounded mb-3">

    <h4 class="text-light border-bottom border-secondary p-2">
      List of feedback</h4>
    <div class="p-2">
      <table class="table table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Attend</th>
          </tr>
        </thead>
        <tbody>

          {{#each attendees}}
            <tr>

              <th scope="row"> {{ @index }} </th>
              <td> <strong>{{this.name}}</strong> </td>
              <td> {{this.email}} </td>
              <td> {{this.willAttend}} </td>
            </tr>

          {{/each}}

        </tbody>
      </table>
    </div>

    <div class="p-2 mb-3">
      <button class="btn bg-primary rounded">
        <a href="/" class="text-light text-decoration-none"> Go home </a>
      </button>
    </div>

  </div>
</div>